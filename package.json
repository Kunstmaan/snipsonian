{
  "name": "@snipsonian/parent",
  "version": "1.3.0",
  "description": "Small re-usable typescript code snippets (parent module)",
  "main": "src/index.js",
  "scripts": {
    "lintScss": "stylelint **/*.scss",
    "lintJs": "eslint *.js",
    "lintTs": "eslint --ext .ts packages *.ts",
    "test": "npm run lintJs && npm run lintTs && jest",
    "jest": "jest --watch --verbose",
    "del-generated": "del packages/*/es/** packages/*/cjs/**",
    "replace-snipsonian-imports": "node bin/code/replaceSnipsonianImports.js",
    "to-es": "npm run del-generated && lerna exec -- tsc --project tsconfig.json && npm run replace-snipsonian-imports",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@kunstmaan/eslint-config": "^1.1.0",
    "@kunstmaan/stylelint-config": "^1.2.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^11.9.4",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-jest": "^24.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.15.3",
    "jest": "^24.0.0",
    "lerna": "^3.13.1",
    "replace-in-file": "^3.4.4",
    "stylelint": "^9.10.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Kunstmaan/snipsonian.git"
  },
  "keywords": [
    "kunstmaan",
    "javascript",
    "code",
    "snippet",
    "snippets",
    "ecmascript",
    "es6",
    "es2015"
  ],
  "author": "Kunstmaan Development",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kunstmaan/snipsonian/issues"
  },
  "homepage": "https://github.com/Kunstmaan/snipsonian#readme",
  "directories": {
    "doc": "doc"
  }
}
